<h1><%= @cat.name %></h1>
<pre> <%= @cat.to_json %> </pre>
<% if @cat.toys.length > 0 %>
<h2>Toys</h2>

<ul>
  <% @cat.toys.each do |toy| %>
    <li><%=toy.name%></li>
  <% end %>
</ul>
<% end %>
<a href="<%= edit_cat_url(@cat) %>"> Edit Cat! </a>
<br>
<a href="<%= new_cat_toy_url(@cat) %>"> Add toy! </a>
<br>
<br>
<a href="<%= cats_url %>">Index</a>